{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "no-any": true,
    "ordered-imports": true,
    "no-console": [
      true,
      "log"
    ],
    "object-literal-sort-keys": {
      "options": "match-declaration-order"
    },
    "interface-name": false,
    "no-unused-variable": true,
    "array-type": [
      true,
      "array"
    ],
    "semicolon": [
      true,
      "always"
    ]
  }
}
